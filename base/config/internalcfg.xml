<?xml version="1.0" encoding="UTF-8"?>
<ipfixcol xmlns="urn:cesnet:params:xml:ns:yang:ipfixcol-internals">

	<!--## List of supported collectors
		## One of these elements is expected in <collectingProcess>
		##   element in startup.xml 
	 -->
	<supportedCollectors>
		<name>sctpCollector</name>
		<name>udpCollector</name>
		<name>tcpCollector</name>
		<name>fileReader</name>
	</supportedCollectors>

	<!--## Input plugin internal configuration -->
	<inputPlugin>
		<!--## Name must match one of the <supportedCollectors> -->
		<name>udpCollector</name>
		<!--## Path of the plugin module -->
		<file>/usr/share/ipfixcol/plugins/ipfixcol-udp-input.so</file>
		<!--## Process name suffix (ipfixcol:UDP) -->
		<processName>UDP</processName>
	</inputPlugin>
	<inputPlugin>
		<name>tcpCollector</name>
		<file>/usr/share/ipfixcol/plugins/ipfixcol-tcp-input.so</file>
		<processName>TCP</processName>
	</inputPlugin>
	<inputPlugin>
		<name>fileReader</name>
		<file>/usr/share/ipfixcol/plugins/ipfixcol-ipfix-input.so</file>
		<processName>file</processName>
	</inputPlugin>
	<inputPlugin>
		<name>sctpCollector</name>
		<file>/usr/share/ipfixcol/plugins/ipfixcol-sctp-input.so</file>
		<processName>SCTP</processName>
	</inputPlugin>

	<!--## Storage plugin internal configuration -->
	<storagePlugin>
		<!--## Definition of file format
		    ## The element <fileFormat> in startup.xml must
		    ##   match this element -->
		<fileFormat>ipfix</fileFormat>
		<!--## Path of the storage module  -->
		<file>/usr/share/ipfixcol/plugins/ipfixcol-ipfix-output.so</file>
		<!--## Thread name suffix (storage:ipfix) -->
		<threadName>ipfix</threadName>
	</storagePlugin>
	<storagePlugin>
		<fileFormat>dummy</fileFormat>
		<file>/usr/share/ipfixcol/plugins/ipfixcol-dummy-output.so</file>
		<threadName>dummy</threadName>
	</storagePlugin>
        <!--## ipfixviewer storage plugin -->
	<storagePlugin>
		<fileFormat>view</fileFormat>
		<file>/usr/share/ipfixcol/plugins/ipfixcol-ipfixviewer-output.so</file>
		<threadName>view</threadName>
	</storagePlugin>
	
	<!-- Intermediate Plugins -->
	<intermediatePlugin>
		<name>dummy_ip</name>
		<file>/usr/share/ipfixcol/plugins/ipfixcol-dummy-inter.so</file>
		<threadName>dummy_inter</threadName>
	</intermediatePlugin>
	
	<intermediatePlugin>
		<name>anonymization_ip</name>
		<file>/usr/share/ipfixcol/plugins/ipfixcol-anonymization-inter.so</file>
		<threadName>anon_inter</threadName>
	</intermediatePlugin>
