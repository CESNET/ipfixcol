<?xml version="1.0" encoding="UTF-8"?>
<ipfix xmlns="urn:ietf:params:xml:ns:yang:ietf-ipfix-psamp">

    <collectingProcess>
		<name>IPFIX collector</name>
		<fileReader>
			<file>file:../ipfix_data/bad-length-in-set-very-long.ipfix</file>
		</fileReader>
		<exportingProcess>File writer</exportingProcess>
	</collectingProcess>


	<exportingProcess>
		<name>File writer</name>
		<destination>
			<name>File writer</name>
			<fileWriter>
				<fileFormat>ipfix</fileFormat>
				<file>file:./out.ipfix</file>
			</fileWriter>
		</destination>
	</exportingProcess>

</ipfix>
